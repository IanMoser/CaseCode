<div id="wrap">
  <form name="myForm" action method="post">
    <select name="categoriesProd" onchange="updateModels(this.form)">
    <option>Please select</option>
    <option>Central</option>
    <option>Mobile</option>
    <option>Monitoring</option>
    <option>Online_Banking</option>
    <option>Q2_Products</option>
    <option>Supporting_Services</option>
    <option>Vendor_-_Third_Party</option>
    <option>All_Channels_Affected</option>
	<option>Other_(please_specify)</option>
    </select>
<br>
<br>
    <heading>PRODUCT DETAILS</heading>
	<br>
<br>
	<select name="prod"></select>
  </form>
</div>




<script type="text/javascript">
var prod = [];
prod.Central = ["Administration","Secure Messaging",
"Reports"];
prod.Mobile = ["Android","iOS","Both"];
prod.Monitoring = ["Alerts Investigation","Outage","App Dynamics","JAMS"];
prod.Online_Banking = ["Account History ","Account List","ACH","Funds Transfer","User Interface","Wedge/Forms","Wires"];
prod.Q2_Products = ["Corporate Banking","Customer Portal","Q2 Assist","Q2 Patrol","Q2 Sentinel","Q2 Smart","Test Environment","Text Banking","Voice Banking"];
prod.Supporting_Services = ["Batch Import","Notifications","TAC","Custom Reports"];
prod.Vendor_-_Third_Party = ["Bill Pay","MRDC","PFM","E-Statements/Check Images"];
prod.All_Channels_Affected = [];
prod.Other_(please_specify) = [];



// ---
function updateModels(theForm) 
 { if(theForm.categoriesProd.selectedIndex<1){return false; }
   var opProd=theForm.categoriesProd.options;
   var newModels = prod[opProd[opProd.selectedIndex].value];
   theForm.prod.options.length = 0;
   theForm.prod.options[0] = new Option("Please select", "");
   theForm.prod.options[0].selected = true;
   for (var i=0; i<newModels.length; i++) 
   { theForm.prod.options[i+1] = new Option(newModels[i], newModels[i]); }  
 }
// ---
// check both selections have been made
 document.getElementById("B1").onclick=function(event)
  { event=event || window.event;
    var targElem1 = event.target || event.srcElement;
    var thisForm1=targElem.parentNode;
    if(thisForm1.prod.selectedIndex>0 && thisForm.prod.selectedIndex>0)
      { var prodP=thisForm.prod;
        location.href= prodP[prodP.selectedIndex].value+".htm"; }
    else { return false; } 
  }
//
</script>



